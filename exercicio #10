notas = [1, 5.5, 7, 9 ]
tamanho = len(notas)
print(f'foram lidas {tamanho} notas ')
print(' '.join([str(nota) for nota in notas]))
notas.reverse()

for nota in notas:
  print(nota)

soma = sum(notas)

print(f'a soma das notas Ã© {soma} ')

media = soma/tamanho

print(f'a media foi: {media} ')

print('notas acima da media: ')
print(' '.join([str(nota) for nota in notas if nota > media ]))

print('notas abaixo da media: ')
print(' '.join([str(nota) for nota in notas if nota < 7 ]))
